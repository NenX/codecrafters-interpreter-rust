fun sayAge() {
  print this.age;
}

class Person {
  sayName() {
    print this.name;
  }
}

var jane = Person();
jane.name = "Jane";
jane.sayName(); // Jane
jane.sayAge = sayAge;
jane.sayAge(); // 20
var bill = Person();
bill.name = "Bill";
bill.sayName(); // Bill

bill.sayName = jane.sayName;
bill.sayName(); // Jane
fun sayAge() {
  print this.age;
}
bill.sayAge = sayAge;
bill.sayAge(); // 20
